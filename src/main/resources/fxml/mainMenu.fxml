<AnchorPane maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" stylesheets="@bootstrap3.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.org.AppController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox layoutX="464.0" layoutY="32.0" prefHeight="93.0" prefWidth="122.0">
               <children>
                  <Label alignment="TOP_CENTER" prefHeight="18.0" prefWidth="127.0" text="Nickname" />
                  <TextField id="nicknameField" fx:id="nicknameField" alignment="CENTER" prefHeight="19.0" prefWidth="147.0" text="player">
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </TextField>
               </children>
            </VBox>
            <VBox alignment="CENTER" layoutX="191.0" layoutY="100.0" prefHeight="192.0" prefWidth="252.0" spacing="35.0">
               <children>
                  <Button id="browseGames" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="38.0" prefWidth="213.0" styleClass="success" text="Browse games" />
